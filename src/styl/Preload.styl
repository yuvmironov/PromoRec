.Preload
	position fixed
	width 100%
	height 100%
	background-color white
	z-index 9999
	display flex
	align-items center
	justify-content center
	&-Hide
		animation HidePreload 0.6s cubic-bezier(0.895, 0.03, 0.685, 0.22) forwards
		
@keyframes HidePreload
	from
		transform translateX(0)
	to
		transform translateX(100%)
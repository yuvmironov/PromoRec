.FullSlide
	&-Text
		font-family: Roboto;
		line-height: 18px;
		font-size: 14px;
		text-align: center;
		color: #859FAB;
		max-width 563px
		margin 0 auto
		&:not(:last-child)
			margin-bottom: 20px
	&-Header
		position: fixed;
		top: 120px;
		left: calc(50% - 399px);
		display inline-block
		border-style: solid;
		border-width: 6px;
		border-image: url('images/Princip/PrincipBorder.svg') 6 fill repeat;
		padding 26px 33px
		margin-bottom 20px
	&-HeadTwo
		position: fixed;
		top: 270px;
		left: calc(50% - 399px);
		font-family: Rubik;
		font-size: 15px;
		text-align: center;
		letter-spacing: 0.05em;
		color: #A2B9C2;
		max-width 799px
		margin 0 auto
		margin-bottom 62px
	&-HeaderText
		font-family: Rubik;
		font-weight: 500;
		font-size: 28px;
		text-align: center;
		letter-spacing: 0.1em;
		color: #A4B9C2;
	&-Image
		display block
		margin 0 auto
		margin-bottom 43px
		margin-top: 110px;
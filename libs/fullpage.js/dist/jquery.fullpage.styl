html.fp-enabled,
.fp-enabled body {
	margin: 0;
	padding: 0;
	overflow:hidden;

	/*Avoid flicker on slides transitions for mobile phones #336 */
	-webkit-tap-highlight-color: rgba(0,0,0,0);
}
.fp-section {
	position: relative;
	-webkit-box-sizing: border-box; /* Safari<=5 Android<=3 */
	-moz-box-sizing: border-box; /* <=28 */
	box-sizing: border-box;
}
.fp-slide {
	float: left;
}
.fp-slide, .fp-slidesContainer {
	height: 100%;
	display: block;
}
.fp-slides {
	z-index:1;
	height: 100%;
	overflow: hidden;
	position: relative;
	-webkit-transition: all 0.3s ease-out; /* Safari<=6 Android<=4.3 */
	transition: all 0.3s ease-out;
}
.fp-section.fp-table, .fp-slide.fp-table {
	display: table;
	table-layout:fixed;
	width: 100%;
}
.fp-tableCell {
	display: table-cell;
	vertical-align: middle;
	width: 100%;
	height: 100%;
	text-align center
}
.fp-slidesContainer {
	float: left;
	position: relative;
}
.fp-controlArrow {
	-webkit-user-select: none; /* webkit (safari, chrome) browsers */
	-moz-user-select: none; /* mozilla browsers */
	-khtml-user-select: none; /* webkit (konqueror) browsers */
	-ms-user-select: none; /* IE10+ */
	position: absolute;
	z-index: 4;
	top: 50%;
	cursor: pointer;
	width: 0;
	height: 0;
	//border-style: solid;
	margin-top: -38px;
	-webkit-transform: translate3d(0,0,0);
	-ms-transform: translate3d(0,0,0);
	transform: translate3d(0,0,0);
}
.fp-controlArrow.fp-prev {
	left: 515px;
	//background-color: rebeccapurple;
	width: 30px;
	height: 50px;
}
.fp-controlArrow.fp-prev:before {
		position: absolute;
		content: "";
		width: 2px;
		height: 30px;
		background-color: #AEC0C8
		transform: rotate(33deg);
		bottom: 23px;
		left: 13px;
}
.fp-controlArrow.fp-prev:after {
		position: absolute;
		content: "";
		width: 2px;
		height: 30px;
		background-color: #AEC0C8
		transform: rotate(-33deg);
		top: 22px;
		left: 13px;
}
.fp-controlArrow.fp-next {
	right: 515px;
	width: 30px;
	height: 50px;
	//border-width: 38.5px 0 38.5px 34px;
	//border-color: transparent transparent transparent #fff;
}
.fp-controlArrow.fp-next:before {
		position: absolute;
		content: "";
		width: 2px;
		height: 30px;
		background-color: #AEC0C8
		transform: rotate(-33deg);
		bottom: 23px;
		left: 13px;
}
.fp-controlArrow.fp-next:after {
		position: absolute;
		content: "";
		width: 2px;
		height: 30px;
		background-color: #AEC0C8
		transform: rotate(33deg);
		top: 22px;
		left: 13px;
}
.fp-scrollable {
	overflow: hidden;
	position: relative;
}
.fp-scroller{
	overflow: hidden;
}
.iScrollIndicator{
	border: 0 !important;
}
.fp-notransition {
	-webkit-transition: none !important;
	transition: none !important;
}
#fp-nav {
	position: fixed;
	z-index: 100;
	margin-top: -200px !important;
	top: 50%;
	opacity: 1;
	-webkit-transform: translate3d(0,0,0);
}
#fp-nav.right {
	right: 17px;
}
#fp-nav.left {
	left: 17px;
}
.fp-slidesNav{
	position: absolute;
	display: flex;
	z-index: 4;
	opacity: 1;
	-webkit-transform: translate3d(0,0,0);
	-ms-transform: translate3d(0,0,0);
	transform: translate3d(0,0,0);
	left: 0 !important;
	right: 0;
	margin: 0 auto !important;
}
.fp-slidesNav.bottom {
	bottom: 150px;
}
.fp-slidesNav.top {
	top: 17px;
}
#fp-nav ul {
  margin: 0;
  padding: 0;
  margin: 0 auto;
}
.fp-slidesNav ul {
	margin: 0;
	padding: 0;
	margin: 0 auto;
	background-image: url(images/Princip/PrincipDotsLine.svg);
	background-repeat: no-repeat;
